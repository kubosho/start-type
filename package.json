{
    "name": "start-type",
    "version": "1.0.0",
    "description": "Boilerplate of development environment using TypeScript.",
    "author": "Shota Kubota <ta2@o2p.jp>",
    "license": "MIT",
    "private": true,
    "scripts": {
        "build": "babel ./src --out-dir ./lib --extensions '.ts,.tsx'",
        "format": "prettier --config .prettierrc.js --write **/*.{js,jsx,ts,tsx}",
        "check_format": "npm run format && git diff --exit-code",
        "lint": "eslint --config .eslintrc.js --ext .ts,.tsx .",
        "test": "jest",
        "update_snapshot": "jest --updateSnapshot",
        "typecheck": "tsc --project ."
    },
    "dependencies": {},
    "devDependencies": {
        "@babel/cli": "^7.8.4",
        "@babel/core": "^7.8.7",
        "@babel/preset-env": "^7.9.5",
        "@babel/preset-typescript": "^7.8.3",
        "@typescript-eslint/eslint-plugin": "^2.6.0",
        "@typescript-eslint/parser": "^2.22.0",
        "eslint": "^6.8.0",
        "eslint-config-prettier": "^6.9.0",
        "eslint-plugin-prettier": "^3.1.2",
        "jest": "^24.9.0",
        "option-t": "^22.2.4",
        "prettier": "^1.18.2",
        "typescript": "^3.8.3"
    }
}
