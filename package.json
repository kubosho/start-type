{
    "name": "start-type",
    "version": "1.0.0",
    "description": "Boilerplate of development environment using TypeScript.",
    "author": "Shota Kubota <ta2@o2p.jp>",
    "license": "MIT",
    "private": true,
    "scripts": {
        "build": "babel ./src --out-dir ./lib --extensions '.ts,.tsx'",
        "format": "prettier --config .prettierrc.js --write **/*.{js,jsx,ts,tsx}",
        "check_format": "npm run format && git diff --exit-code",
        "lint": "eslint --config .eslintrc.js --ext .ts,.tsx .",
        "test": "jest",
        "update_snapshot": "jest --updateSnapshot",
        "typecheck": "tsc --project ."
    },
    "dependencies": {},
    "devDependencies": {
        "@babel/cli": "^7.14.5",
        "@babel/core": "^7.14.6",
        "@babel/preset-env": "^7.14.7",
        "@babel/preset-typescript": "^7.14.5",
        "@typescript-eslint/eslint-plugin": "^4.28.2",
        "@typescript-eslint/parser": "^4.28.2",
        "eslint": "^7.30.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^3.4.0",
        "jest": "^29.2.2",
        "option-t": "^28.1.1",
        "prettier": "^2.3.2",
        "typescript": "^4.3.5"
    }
}
