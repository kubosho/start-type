{
    "name": "start-type",
    "version": "1.0.0",
    "description": "Boilerplate of development environment using TypeScript.",
    "author": "Shota Kubota <ta2@o2p.jp>",
    "license": "MIT",
    "private": true,
    "scripts": {
        "build": "babel ./src --out-dir ./lib --extensions '.ts,.tsx'",
        "format": "prettier --config .prettierrc.js --write **/*.{js,jsx,ts,tsx}",
        "check_format": "npm run format && git diff --exit-code",
        "lint": "eslint --config .eslintrc.js --ext .ts,.tsx .",
        "test": "jest",
        "update_snapshot": "jest --updateSnapshot",
        "typecheck": "tsc --project ."
    },
    "dependencies": {},
    "devDependencies": {
        "@babel/cli": "^7.12.10",
        "@babel/core": "^7.12.10",
        "@babel/preset-env": "^7.12.11",
        "@babel/preset-typescript": "^7.12.7",
        "@typescript-eslint/eslint-plugin": "^2.34.0",
        "@typescript-eslint/parser": "^2.34.0",
        "eslint": "^6.8.0",
        "eslint-config-prettier": "^6.15.0",
        "eslint-plugin-prettier": "^3.3.0",
        "jest": "^26.6.3",
        "option-t": "^24.1.0",
        "prettier": "^2.2.1",
        "typescript": "^4.1.3"
    }
}
